<div class="m-4 p-4">
  <%= simple_form_for(@artwork) do |f| %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.input :shipping_costs %>
    <%= f.input :photos, as: :file, input_html: { multiple: true } %>
    <span class=""><%= f.submit %></span>
  <% end %>
</div>
